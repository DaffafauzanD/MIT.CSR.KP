@{
    MIT.ECSR.Shared.Attributes.TokenUserObject token = ViewBag.User;
    ViewData["Title"] = "Penawaran";
    bool isVendor = token != null && token.Role.Name.Contains(MIT.ECSR.Shared.Attributes.RoleName.FORUM.ToString());
}
@if (isVendor)
{
    @await Html.PartialAsync("~/Views/Penawaran/External/Index.cshtml",token)
}
else
{
    @await Html.PartialAsync("~/Views/Penawaran/Internal/Index.cshtml",token)
}

@section scripts{
    @if (isVendor)
    {
        <script type="text/javascript" src="@Url.Content("~/Scripts/penawaran/external/index.js")"></script>
    }
    else
    {
        <script type="text/javascript" src="@Url.Content("~/Scripts/penawaran/internal/index.js")"></script>

    }
}