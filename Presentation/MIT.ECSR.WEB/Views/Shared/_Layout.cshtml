@{
    MIT.ECSR.Shared.Attributes.TokenUserObject Token = ViewBag.User;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="MIT CSR">
    <meta name="author" content="ILYAS">
    <title>@ViewData["Title"] - E CSR</title>
    <link rel="apple-touch-icon" href="@Url.Content("~/Content/app-assets/images/ico/apple-icon-120.png")">
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/Content/app-assets/images/ico/favicon.ico")">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Quicksand:300,400,500,700" rel="stylesheet">
    <link href="https://maxcdn.icons8.com/fonts/line-awesome/1.1/css/line-awesome.min.css" rel="stylesheet">

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/vendors.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/app.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/core/menu/menu-types/horizontal-menu.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/core/colors/palette-gradient.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/fonts/simple-line-icons/style.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/extensions/toastr.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/plugins/extensions/toastr.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/extensions/sweetalert.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/timeline/timeline.min.css")">

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/dropzone/dropzone.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/pickers/daterange/daterangepicker.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/vendors/css/pickers/pickadate/pickadate.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/app-assets/css/plugins/pickers/daterange/daterange.min.css")" />

    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/assets/css/style.css")">
    @RenderSection("Styles", false)
</head>

<body class="horizontal-layout horizontal-menu 2-columns menu-expanded" data-open="hover" data-menu="horizontal-menu" data-col="2-columns">
    <!-- fixed-top-->
    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow navbar-static-top navbar-light navbar-brand-center">
        <div class="navbar-wrapper">

            @await Html.PartialAsync("~/Views/Shared/_Header.cshtml", Token)
        </div>
    </nav>

    @await Html.PartialAsync("~/Views/Shared/_Menu.cshtml", Token)

    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                @RenderBody()
            </div>
        </div>
    </div>

    <footer class="footer footer-static footer-light navbar-shadow">
        <p class="clearfix blue-grey lighten-2 text-sm-center mb-0 px-2">
            <span class="float-md-left d-block d-md-inline-block">
                Copyright &copy; 2022 <a class="text-bold-800 grey darken-2" href="http://mediaindoteknologi.com"
                                         target="_blank">Media Indo Teknologi </a>, All rights reserved.
            </span>
            <span class="float-md-right d-block d-md-inline-blockd-none d-lg-block">Hand-crafted & Made with <i class="ft-heart pink"></i></span>
        </p>
    </footer>


    @RenderSection("modals", false)

    @await Html.PartialAsync("~/Views/Shared/_Login.cshtml")
    @await Html.PartialAsync("~/Views/Shared/_Profil.cshtml")

    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/vendors/js/vendors.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/vendors/js/ui/jquery.sticky.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/vendors/js/charts/jquery.sparkline.min.js")"></script>

    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/core/app-menu.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/core/app.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/scripts/customizer.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/vendors/js/extensions/toastr.min.js")"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/extensions/sweetalert.min.js")" type="text/javascript"></script>

    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/scripts/ui/breadcrumbs-with-stats.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/waitMe/waitMe.min.js")"></script>


    <script src="@Url.Content("~/Content/app-assets/vendors/js/dropzone/dropzone.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/dateTime/moment-with-locales.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/daterange/daterangepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/pickadate/picker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/pickadate/picker.date.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/pickadate/picker.time.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/pickers/pickadate/legacy.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/app-assets/vendors/js/echarts/echarts-en.min-440rc1.js")" type="text/javascript"></script>

    @*<script src="https://unpkg.com/dropzone@5/dist/min/dropzone.min.js"></script>*@

    @*<script type="text/javascript" src="@Url.Content("~/Content/app-assets/js/timeline/horizontal-timeline.js")"></script>*@

    <script type="text/javascript" src="@Url.Content("~/Scripts/helper.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/date_helper.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/request.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/account.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/referensi_list.js")"></script>
    @RenderSection("Scripts", false)
</body>
</html>